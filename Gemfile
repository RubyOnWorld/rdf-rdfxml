group :development do
  gem "rdf-spec",       :git => "git://github.com/ruby-rdf/rdf-spec.git"
  gem "rdf-turtle",       :git => "git://github.com/ruby-rdf/rdf-turtle.git"
end

group :test do
  gem "wirble"
  gem "debugger", :platforms => [:mri_19]
end
